<%if flash[:error] %>
  <% flash[:error].each do |e| %>
    <p><%= e %></p>
  <%end %>
<%end %>
<%= form_for @dragon do |f| %>
  <%= f.label "Name:" %>
  <%= f.text_field :name %>
  <br/>
  <%= f.label "Age:" %>
  <%= f.number_field :age %>
  <br/>
  <%= f.label "Current Master" %>
  <%= f.collection_select :user_id, @users, :id, :name %>
  <br/>
  <%= f.submit %>
<%end %>
